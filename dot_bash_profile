# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
	. ~/.bashrc
fi

if [ -z "$SSH_AUTH_SOCK" ] ; then
    eval `ssh-agent -s` &> /dev/null
    ssh-add -q
fi

# User specific environment and startup programs
export VISUAL=nvim
export EDITOR=nvim
export MOZ_ENABLE_WAYLAND=1
export QT_QPA_PLATFORM=wayland
